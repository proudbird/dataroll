{
  "compileOnSave": true,
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "ES2020",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "sourceMap": true,
      "rootDir": "src", 
      "outDir": "./build/",
      "baseUrl": ".",
      "paths": {
          "*": [
              "."
          ]
      }
  },
  "include":["**/*.ts","**/*.json", "src/descriptValue"],
  "exclude": ["build", "tsconfig.json"]
}